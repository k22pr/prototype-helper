{
  "name": "prototype-helper",
  "version": "0.4.2",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k22pr/prototype-helper.git"
  },
  "keywords": [
    "typescript",
    "prototype",
    "extension",
    "override",
    "helper"
  ],
  "author": "서버지기",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k22pr/prototype-helper/issues"
  },
  "homepage": "https://github.com/k22pr/prototype-helper#readme",
  "dependencies": {
    "decimal.js": "^10.4.1",
    "deep-clone": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.31",
    "@types/prettier": "^2.7.1",
    "jest": "^27.5.1",
    "nodemon": "^1.19.4",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.5",
    "ts-loader": "^6.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "jest && tsc",
    "dev": "nodemon --exec ts-node ./src/test.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "publish": "npm publish"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "src"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
